<h1>Hello</h1>
<% @users.each do |user| %>
    <li class="article-title">
        <%=link_to user.username, user_path(user) %>
    </li>
<% if current_user.super? %>
<% if user.admin?%>
<%= link_to 'Remove Admin', deactive_user_path(user) %>
<%else%>
<%= link_to 'Make Admin', active_user_path(user) %> 
<%end%>
<%end%>
<%end%>

